@{
    ViewData["Title"] = $"{Model.Name}";
    Layout = "~/Areas/AdminPanel/Views/Shared/_AdminLayout.cshtml";
}

@model Terminal

<div class="col-md-12 grid-margin stretch-card">
    <div class="card">
        <div class="card-body">
            <h4 class="card-title">Terminal</h4>
            <p class="card-description">
                @Model.Name
            </p>
            <p>
                @Model.Address
            </p>
        </div>
    </div>
</div>
